{
  "roots": ["<rootDir>"],
  "testMatch": ["**/*.integration.test.ts","**/*.unit.test.ts"],
  "extensionsToTreatAsEsm": [".ts"],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "transform": {
    "^.+\\.tsx?$": [
      "ts-jest",
      {
        "useESM": true
      }
    ]
  },
  "testTimeout": 10000
}
